{
  "name": "@topechelon/bb-schematics",
  "version": "1.0.0",
  "description": "TE Angular Schematics",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "example": "npm run clean && npm run example:module && npm run example:component",
    "example:module": "schematics .:module shared/network --debug=false",
    "example:component": "schematics .:component shared/network/recruiter-profile/image --debug=false",
    "clean": "rm -rf src/app/"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^11.0.0",
    "@angular-devkit/schematics": "^11.0.0",
    "@schematics/angular": "^11.0.1",
    "typescript": "~4.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "^3.5.0"
  }
}
