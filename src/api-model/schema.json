{
  "$schema": "http://json-schema.org/schema",
  "id": "BBApiModelSchematics",
  "title": "BB API Model Options Schema",
  "type": "object",
  "description": "Creates a BB API Model",
  "properties": {
    "path": {
      "type": "string",
      "description": "The path of the model",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the model's path? (e.g. network/discussion/comment)"
    }
  },
  "required": [
    "path"
  ]
}
