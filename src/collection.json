{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "component": {
      "description": "BB Component Schematic",
      "factory": "./component/index#component",
      "schema": "./component/schema.json"
    },
    "module": {
      "description": "BB Module Schematic",
      "factory": "./module/index#module",
      "schema": "./module/schema.json"
    },
    "service": {
      "description": "BB Service Schematic",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "upgrade": {
      "description": "BB Upgrade AJS Schematic",
      "factory": "./upgrade/index#upgrade",
      "schema": "./upgrade/schema.json"
    },
    "api": {
      "description": "BB API Model & Service Schematic",
      "factory": "./api/index#api",
      "schema": "./api/schema.json"
    },
    "api-model": {
      "description": "BB API Model Schematic",
      "factory": "./api-model/index#apiModel",
      "schema": "./api-model/schema.json"
    },
    "api-service": {
      "description": "BB API Service Schematic",
      "factory": "./api-service/index#apiService",
      "schema": "./api-service/schema.json"
    },
    "pipe": {
      "description": "BB Pipe Schematic",
      "factory": "./pipe/index#pipe",
      "schema": "./pipe/schema.json"
    },
    "directive": {
      "description": "BB Directive Schematic",
      "factory": "./directive/index#directive",
      "schema": "./directive/schema.json"
    }
  }
}
